<h3>Search result</h3>


<div class="container col-sm-9" id="jobs">
    <%= turbo_frame_tag "job" do %>
        <% if @jobs.any? %>
            <% @jobs.each do |job| %>
                <%= render job %>
            <% end %>
        <% else %>
            <h3> Search not found </h3>
        <% end %>
    <% end %>
</div>
